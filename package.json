  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "backend/**/*.js",
      "data/**/*.js",
      "common/**/*.js",
      "js/**/*.js"
    ],
    "transform": {
      "\\.graphql$": "<rootDir>/scripts/jest/graphqlPreprocessor",
      "^.+\\.js$": "<rootDir>/scripts/jest/jsPreprocessor"
    },
    "setupFiles": [
      "<rootDir>/scripts/jest/setup",
      "<rootDir>/node_modules/babel-polyfill",
      "node_modules/fbjs-scripts/jest/environment.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
