{
  "name": "b2b-trading-platform",
  "version": "1.0.0",
  "description": "B2B Trading Platform",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build backend server js common data scripts",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "better-npm-run test --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "compile": {
      "command": "node -r server/env bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --ignore dist --ignore js -r server/env bin/server.js --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node -r server/env bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "engines": {
    "node": ">=6.x.x <7.0.0",
    "yarn": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "bitbucket:amsayk/b2b-trading-platform"
  },
  "keywords": [
    "node",
    "b2b",
    "graphql",
    "react",
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript"
  ],
  "author": "Amadou Cisse <cisse.amadou.9@gmail.com>",
  "contributors": [],
  "homepage": "https://bitbucket.org/amsayk/b2b-trading-platform#readme",
