{
  "name": "b2b-trading-platform",
  "version": "0.1.0",
  "description": "B2B Trading Platform",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "npm run stylelint && npm run jslint",
    "jslint": "npm run update-schema && eslint bin build backend server js common data scripts",
    "jslint:fix": "npm run jslint -- --fix",
    "stylelint": "stylelint 'js/**/*.scss'; exit 0",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "better-npm-run test --watch",
    "test:coverage": "better-npm-run test --coverage",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "translate": "better-npm-run translate",
    "update-schema": "better-npm-run update-schema"
  },
  "betterScripts": {
    "update-schema": {
      "command": "node -r server/env bin/updateSchema.js",
      "env": {}
    },
    "compile": {
      "command": "node -r server/env bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --watch server --watch backend --watch data --watch common -r server/env bin/dev-server.js --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node -r server/env bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "translate": {
      "command": "node -r server/env bin/translate",
      "env": {}
    }
  },
  "engines": {
    "node": ">=6.x.x <7.0.0",
    "yarn": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "bitbucket:amsayk/b2b-trading-platform"
  },
  "keywords": [
    "node",
    "b2b",
    "graphql",
    "react",
    "ecmascript",
    "es2015",
    "jsnext",
    "javascript"
  ],
  "author": "Amadou Cisse <cisse.amadou.9@gmail.com>",
  "contributors": [],
  "homepage": "https://bitbucket.org/amsayk/b2b-trading-platform#readme",
