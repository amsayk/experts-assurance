{
  "name": "fikrat",
  "version": "0.1.6",
  "description": "Experts d'assurance",
  "private": true,
  "pre-commit": [
    "lint",
    "test"
  ],
  "scripts": {
    "persistgraphql": "node --stack-size=8192 $(npm bin)/persistgraphql js persisted_queries.json --add_typename",
    "clean": "rimraf dist",
    "precompile": "npm run clean",
    "compile": "npm run persistgraphql && NODE_PATH=.:./js bnr compile",
    "lint": "npm run stylelint && npm run jslint",
    "jslint": "npm run update-schema && eslint bin build backend server js common data scripts",
    "jslint:fix": "npm run jslint -- --fix",
    "stylelint": "stylelint 'js/**/*.scss'; exit 0",
    "start": "NODE_PATH=.:./js bnr start",
    "main_worker": "NODE_PATH=.:./js bnr main_worker",
    "es_index": "NODE_PATH=.:./js bnr es_index",
    "auth_worker": "NODE_PATH=.:./js bnr auth_worker",
    "mail": "NODE_PATH=.:./js bnr mail",
    "dev": "NODE_PATH=.:./js bnr dev",
    "dev:main_worker": "NODE_PATH=.:./js bnr dev:main_worker",
    "dev:es_index": "NODE_PATH=.:./js bnr dev:es_index",
    "dev:auth_worker": "NODE_PATH=.:./js bnr dev:auth_worker",
    "dev:mail": "NODE_PATH=.:./js bnr dev:mail",
    "test": "NODE_PATH=.:./js bnr test",
    "test:dev": "NODE_PATH=.:./js bnr test --watch",
    "test:coverage": "NODE_PATH=.:./js bnr test --coverage",
    "deploy": "bnr deploy",
    "deploy:dev": "bnr deploy:dev",
    "deploy:prod": "bnr deploy:prod",
    "translate": "NODE_PATH=.:./js bnr translate",
    "update-schema": "NODE_PATH=.:./js bnr update-schema",
    "import-samples": "NODE_PATH=.:./js bnr import-samples",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "betterScripts": {
    "update-schema": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/update-schema",
      "env": {}
    },
    "import-samples": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/import-samples",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --watch backend --watch server --watch data --watch common -r server/utils/validate-env-vars -r server/env bin/www --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,ioredis:cluster,apollo-local,-app:server:history"
      }
    },
    "dev:main_worker": {
      "command": "nodemon --watch backend --watch common -r server/utils/validate-env-vars -r server/env bin/main_worker --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,ioredis:cluster"
      }
    },
    "dev:es_index": {
      "command": "nodemon --watch backend --watch common -r server/utils/validate-env-vars -r server/env bin/es_index --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,ioredis:cluster"
      }
    },
    "dev:auth_worker": {
      "command": "nodemon --watch backend --watch common -r server/utils/validate-env-vars -r server/env bin/auth_worker --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,ioredis:cluster"
      }
    },
    "dev:mail": {
      "command": "nodemon --watch backend --watch common -r server/utils/validate-env-vars -r server/env bin/mail --exec node",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*,ioredis:cluster"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/www",
      "env": {}
    },
    "test": {
      "command": "node -r server/utils/validate-env-vars node_modules/jest-cli/bin/jest.js",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "translate": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/translate",
      "env": {}
    },
    "main_worker": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/main_worker",
      "env": {
        "DEBUG": "ioredis:cluster"
      }
    },
    "es_index": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/es_index",
      "env": {
        "DEBUG": "ioredis:cluster"
      }
    },
    "auth_worker": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/auth_worker",
      "env": {
        "DEBUG": "ioredis:cluster"
      }
    },
    "mail": {
      "command": "node -r server/utils/validate-env-vars -r server/env bin/mail",
      "env": {
        "DEBUG": "ioredis:cluster"
      }
    }
  },
